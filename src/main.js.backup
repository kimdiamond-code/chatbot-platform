import React from 'react'
import ReactDOM from 'react-dom/client'

function App() {
  return React.createElement('div', {
    style: { 
      padding: '50px', 
      textAlign: 'center',
      fontFamily: 'Arial, sans-serif'
    }
  }, [
    React.createElement('h1', {
      key: 'title',
      style: { 
        color: 'green', 
        fontSize: '48px',
        margin: '20px 0'
      }
    }, 'âœ… REACT IS WORKING!'),
    React.createElement('p', {
      key: 'text',
